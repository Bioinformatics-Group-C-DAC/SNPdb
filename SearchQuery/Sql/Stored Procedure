DROP PROCEDURE IF EXISTS proc_IN;
CREATE PROCEDURE proc_IN (IN input_param_1 VARCHAR(10),IN input_param_2 VARCHAR(10),IN input_param_3 VARCHAR(10))
BEGIN
	declare tble1 varchar(20)	;
	declare tble2 varchar(20)	;
	declare tble3 varchar(20)	;
	
	
        set @tble1 = input_param_1;
        set @tble2 = input_param_2;
        set @tble3 = input_param_3;



	select t.*,t2.ALT as M1_ALT from (select c.Chromosome_Position, d.ChickenLine_Name ,e.Chromosome,c.ALT,c.Ref from Common_ChickenLines c, ChickenLine_Info d ,ChickenLine_Chromosome_Info e where (c.ChickenLine_ID = d.ChickenLine_ID and d.ChickenLine_Name =@tble2) and c.Chromosome_ID = e.Chromosome_ID) as t inner join (select c.Chromosome_Position, d.ChickenLine_Name,e.Chromosome,c.ALT,c.Ref from Common_ChickenLines c, ChickenLine_Info d ,ChickenLine_Chromosome_Info e where (c.ChickenLine_ID = d.ChickenLine_ID and d.ChickenLine_Name =@tble1) and c.Chromosome_ID = e.Chromosome_ID) as t2 where t.Chromosome_Position = t2.Chromosome_Position and t.ALT != t2.ALT;
	
	select @tble1nm;
END//
